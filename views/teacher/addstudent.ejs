<h2>Add a new Student</h2>
<form method="POST" class="addStu" autocomplete="off">
    <div class="mb-3">
      <label for="exampleInputEmail1" class="form-label">Name : </label>
      <input type="text" name="name" required class="form-control" id="exampleInputEmail1">
    </div>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Roll No. : </label>
        <input type="number" name="roll" required class="form-control" id="exampleInputEmail1" >
      </div>
      <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Data Structures : </label>
        <input type="number" name="math" class="form-control" id="exampleInputEmail1" >
      </div>
      <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">OOPS : </label>
        <input type="number" name="english" class="form-control" id="exampleInputEmail1" >
      </div>
      <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Computer Architecture : </label>
        <input type="number" name="science" class="form-control" id="exampleInputEmail1">
      </div>
      <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Principles of Communication : </label>
        <input type="number" name="ss" class="form-control" id="exampleInputEmail1">
      </div>
      <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Discrete Mathematics : </label>
        <input type="number" name="lang" class="form-control" id="exampleInputEmail1">
      </div>
      <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Pass/Fail : </label>
        <input type="text" name="status" required class="form-control" id="exampleInputEmail1">
      </div>
    <button type="submit" class="btn btn-success">Add</button>
    <a href="/teacher/viewall" class="btn btn-primary">View All</a>
</form>